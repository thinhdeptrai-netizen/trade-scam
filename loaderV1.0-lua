local function Toggle(btn, baseText)
	local on = false
	btn.MouseButton1Click:Connect(function()
		on = not on
		btn.Text = baseText .. (on and " üü¢" or " üî¥")
		btn.BackgroundColor3 = on and Color3.fromRGB(0,255,0) or Color3.fromRGB(0,0,0)
	end)
end

