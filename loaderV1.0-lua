local function NeonBorder(frame)
	local stroke = Instance.new("UIStroke")
	stroke.Thickness = 3
	stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	stroke.Parent = frame

	task.spawn(function()
		while true do
			for h = 0, 1, 0.01 do
				stroke.Color = Color3.fromHSV(h, 1, 1)
				task.wait(0.02)
			end
		end
	end)
end

